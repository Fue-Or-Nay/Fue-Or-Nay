<h2 class="profUN">{{user.username}}</h2>
    <main class="prof">
        <div class="profPic">
            <img class="circle responsive-img" src="./public/Images/ntnqruv8nhq1avryljcd.jpeg">
        </div>

        <div class="profInfo">
            <div class="info">
                <p class="revTitle">'{{user.reputation_level}}'</p>
                <p>{{user.bio}}</p>
                <p>Total Reviews: {{user.review_count}}</p>
                <p>Total Feedback Given: {{user.feedback_count}}</p>
            </div>
            <h4 class="fueScore">Your Fue Score:</h4>
            <div class="badges">
                <i class="medium material-icons pink-text lighten-5">whatshot</i>
                <i class="medium material-icons pink-text lighten-2">whatshot</i>
                <i class="medium material-icons pink-text lighten-2">whatshot</i>
                <i class="medium material-icons pink-text lighten-2">whatshot</i>
                <i class="medium material-icons pink-text lighten-2">whatshot</i>
            </div>
        </div>


    </main>
    <br>
    <h3 class="rtdGames">Your Rated Games</h3>
    <div class="selHold">
        <div class="carousel">
           {{#each user.games}}
           {{> user-games}}
           {{/each}}
        </div>
    </div>

    <h3 class="rtdGames">Your Top Reviews:</h3>
    <div class="yoRev">
        <ul class="pagination">
            <li class="disabled"><a href="#!"><i class="material-icons">chevron_left</i></a></li>
            <li class="active blue"><a href="#!">1</a></li>
            <li class="waves-effect"><a href="#!">2</a></li>
            <li class="waves-effect"><a href="#!">3</a></li>
            <li class="waves-effect"><a href="#!">4</a></li>
            <li class="waves-effect"><a href="#!">5</a></li>
            <li class="waves-effect"><a href="#!"><i class="material-icons">chevron_right</i></a></li>
        </ul>
        <div class="rev">
            <h5 class="revGme">"Reviewed Game"</h5>
